password = "simonsnyc"
user_input= input("Enter password") 
num_guess = 0
while num_guess < 3 and user_input!=password:
    print("Wrong Password!")
    num_guess +=1
    if num_guess < 3:
        user_input= input("Enter Password")
    elif user_input==password:
        print("Correct! You may enter...")
    
    else:
        print("End")
            
   

