import math
name= input("What's your name?")
a= int(input("Please enter any a value"))
b= int(input("Please enter any b value"))
c= int(input("Please anter any c value"))

calc1= 4 * a * c
calc2= b ** 2 - calc1
square= math.sqrt(calc2)
 
b2= -b
add= (b2 + square)/(2*a)
subtract= (b2 - square)/(2*a)

print(str(a)+"x^2","+",str(b)+"x","+",str(c)+":","x="+str(add),"&","x="+str(subtract))
                        
